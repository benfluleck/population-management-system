    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "connect-redis": "^3.4.0",
    "coveralls": "^3.0.2",
    "dotenv": "^7.0.0",
    "eslint": "^5.9.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "mongoose": "^5.3.15",
    "nyc": "^13.1.0",
    "pg": "^7.6.1",
